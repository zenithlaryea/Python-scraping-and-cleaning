    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "\n",
    "# Should have made a list and substituted them in the url so it's\n",
    "# all one big for loop. \n",
    "\n",
    "url = \"https://firstnam.es/equatorial-guinea\"\n",
    "\n",
    "response = requests.get(url)\n",
    "\n",
    "# Check request success\n",
    "\n",
    "if response.status_code == 200:\n",
    "    \n",
    "    # Parse the HTML content of the page\n",
    "    \n",
    "    soup = BeautifulSoup(response.text, 'html.parser')\n",
    "    \n",
    "    # Find all <li> elements with class 'list-item'\n",
    "    #Found the li class manually using the inspect page. Not sure\n",
    "    #if there's an automated method\n",
    "    \n",
    "    list_items = soup.find_all('li', class_='list-item col-lg-6 mb-1')\n",
    "    \n",
    "    # Create lists to store data\n",
    "    \n",
    "    names = []\n",
    "    counts = []\n",
    "    images = []\n",
    "    country = []\n",
    "    \n",
    "    # Iterate over each <li> element\n",
    "    \n",
    "    for item in list_items:\n",
    "        # Find the <a> tag within the <li> element\n",
    "        \n",
    "        name_tag = item.find('a')\n",
    "        # Extract the text content of the <a> tag (name)\n",
    "        name = name_tag.text.strip()\n",
    "        names.append(name)\n",
    "        \n",
    "        # Extract the popularity\n",
    "        count = item.contents[-2].strip('() ')\n",
    "        counts.append(count)\n",
    "        \n",
    "        # Extract the image source for the genders\n",
    "        img_tag = item.find('img')\n",
    "        img_src = img_tag['src']\n",
    "        images.append(img_src)\n",
    "        \n",
    "        # Append the country, Manually did this every country\n",
    "        #Should have used a list \n",
    "        \n",
    "        country.append(\"Guinea\")\n",
    "    \n",
    "    # Create a DataFrame\n",
    "    df = pd.DataFrame({\n",
    "        'Name': names,\n",
    "        'Popularity': counts,\n",
    "        'Image': images,\n",
    "        'Country': country\n",
    "    })\n",
    "    \n",
    "    # Print the DataFrame\n",
    "    print(df)\n",
    "else:\n",
    "    print(\"Failed to retrieve the webpage.\")\n"
   ]
  },
