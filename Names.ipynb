    "def sex(image_src):\n",
    "    if 'nino' in image_src:\n",
    "        return 'Male'\n",
    "    elif 'nina' in image_src:\n",
    "        return 'Female'\n",
    "    else:\n",
    "        return 'Unknown'\n",
    "\n",
    "def apply_sex_to_dataframe(df):\n",
    "    df['Gender'] = df['Image'].apply(sex)\n",
    "\n",
    "apply_sex_to_dataframe(df)\n",
    "df"
   ]
